<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Viper GTS-R" type="template">
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.5" max="0.65" val="0.61"/>
		<attnum name="max pressure" unit="kPa" min="5000" max="40000" val="38600"/>
	</section>
	
	<section name="private">
		<attnum name="fuelperlap" val="1.82"/>
		<attnum name="base speed" val="8.80"/>
		<attnum name="base brake" val="15.00"/>
		<attnum name="exit boost factor" val="1.95"/>
		<attnum name="avoid speed factor" val="0.98"/>
		<attnum name="avoid brake factor" val="0.90"/>
		<attnum name="scale brake" val="1.15"/>
		<attnum name="tcl slip" val="2.5"/>
		<attnum name="abs slip" val="1.5"/>
	</section>

</params>














