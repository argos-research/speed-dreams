<!DOCTYPE params SYSTEM "params.dtd">


<params name="sc-murasama-nsx">
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.5" max="0.65" val="0.5927"/>
		<attnum name="max pressure" unit="kPa" min="5000" max="40000" val="31000"/>
	</section>
	
	<section name="private">
		<attnum name="fuelperlap" val="1.05"/>
		<attnum name="avoid speed factor" val="0.85"/>
