		<attnum name="slope factor" val="0.49"/>
		<attnum name="accel exit" val="0.4"/>
		<attnum name="turn decel" val="0.4"/>
		<attnum name="revs change up" val="0.90"/>
		<attnum name="revs change down" val="0.70"/>
		<attnum name="revs change down max" val="0.74"/>
		<attnum name="int margin" min="0" max="2" val="1.7"/>
		<attnum name="ext margin" min="0" max="2" val="1.3"/>
		<attnum name="abs slip" val="4"/>
		<attnum name="tcl slip" val="4"/>
		<attnum name="skid correction" val="0.5"/>
		<attnum name="bump caution" val="0.4"/>
		<attnum name="oversteer asr" val="0.2"/>
	</section>
