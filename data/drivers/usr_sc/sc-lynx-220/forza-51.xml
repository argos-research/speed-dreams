<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="sc-lynx-220" type="template">
	<section name="Features">
		<attstr name="enable abs" in="yes,no" val="yes"/>
		<attstr name="enable tcl" in="yes,no" val="yes"/>
		<attstr name="enable esp" in="yes,no" val="yes"/>
	</section>
	
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.5" max="0.65" val="0.59"/>
		<attnum name="max pressure" unit="kPa" min="5000" max="40000" val="39400"/>
	</section>
	
	<section name="private">
		<attnum name="fuelperlap" val="2.67"/>
		<attnum name="base speed" val="9.10"/>
		<attnum name="base brake" val="8.20"/>
		<attnum name="exit boost factor" val="0.90"/>
		<attnum name="avoid speed factor" val="0.80"/>
		<attnum name="avoid brake factor" val="0.70"/>
		<attnum name="offtrack allowed" val="0"/>
	</section>

</params>














