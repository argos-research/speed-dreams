<!DOCTYPE params SYSTEM "params.dtd">


<params name="sc-cavallo-360">
  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.65" val="0.3235"/>
    <attnum name="max pressure" unit="kPa" min="5000" max="50000" val="40700"/>
    <attnum name="brake corr lr" min="0" max="0.5" val="0.31275"/>
    <attnum name="brake corr fr" min="0" max="0.5" val="0.0987"/>
  </section>

  <section name="simplix private">
    <attnum name="genetic optimisation" min="0" max="1" val="0"/>
    <attnum name="initial fuel" min="-1" max="95" val="-1"/>
    <attnum name="scale brake" min="0.5" max="1.5" val="1.2"/>
    <attnum name="scale mu" min="0.5" max="1.5" val="0.87"/>
    <attnum name="scale bump" min="0.001" max="0.6" val="0.119"/>
    <attnum name="base mode" min="0.97" max="1.03" val="1.01225"/>
    <attnum name="base scale" min="1" max="1.05" val="1.00576"/>
    <attnum name="lookaheadfactor" min="0.01" max="0.09" val="0.09"/>
    <attnum name="side mu" min="0.8" max="1" val="0.95"/>
    <attnum name="side brake" min="0.8" max="1" val="0.95"/>
    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="0"/>
    <attnum name="reserve" min="0" max="20000" val="2000"/>
    <attnum name="fuelper100km" min="1" max="100" val="70"/>
    <attnum name="avoid width" min="0" max="3" val="1.0"/>
    <attnum name="scale avoid" min="1" max="20" val="10"/>
    <attnum name="scale bump outer" min="0" max="2" val="0.6"/>
    <attnum name="scale min mu" min="0.1" max="1" val="0.95"/>
    <attnum name="scale steer" min="0.1" max="2" val="0.5"/>
    <attnum name="stay together" min="10" max="2000" val="10"/>
    <attnum name="length margin" min="1" max="10" val="2"/>
  </section>

</params>
